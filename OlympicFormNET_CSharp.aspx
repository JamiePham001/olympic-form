        // Define DateTime to 'dDate'
        DateTime dDate;
        // Date format is stored in a dictionary
        string[] dateFormat = { "ddMMyyyy" };


        // Function runs validations to ensure YearOfGames input is valid and outputs a boolean value
        bool VALIDATE_YEAR(string year)
        {
            bool invalidyear = false;

            if (string.IsNullOrEmpty(year))
            {
                invalidyear = true;
                Response.Write("<br>Year of Games cannot be empty");
            }
            else if ((year.Length) > 4 || (year.Length) < 4)
            {
                invalidyear = true;
                Response.Write("<br>Year of Games must contain 4 digits");
            }
            else if (!Regex.IsMatch(year, @"^[0-9]+$"))
            {
                invalidyear = true;
                Response.Write("<br>Year of Games must only contain digits");
            }
            return invalidyear;
        }

        // Function runs validations to ensure city input is valid and outputs a boolean value
        bool VALIDATE_CITY(string city)
        {
            bool invalidcity = false;

            if (string.IsNullOrEmpty(city))
            {
                invalidcity = true;
                Response.Write("<br>Must provide a city");
            }
            else if (!Regex.IsMatch(city, @"^[a-zA-Z]+$"))
            {
                invalidcity = true;
                Response.Write("<br>City of Games must only contain letters");
            }
            return invalidcity;
        }

        // Function runs validations to ensure commence and end dates are valid and outputs a boolean value
        bool VALIDATE_DATE(string commence, string end)
        {
            bool invaliddate = false;

            if (string.IsNullOrEmpty(commence))
            {
                invaliddate = true;
                Response.Write("<br>Must provide a commence date");
            }
            else if ((DateTime.TryParseExact(commence, dateFormat, null, System.Globalization.DateTimeStyles.None, out dDate)))
            {
                invaliddate = true;
                Response.Write("<br>Commence date format must be 'dd/mm/yyyy'");
            }
            else if (string.IsNullOrEmpty(end))
            {
                invaliddate = true;
                Response.Write("<br>Must provide a end date");
            }
            else if ((DateTime.TryParseExact(end, dateFormat, null, System.Globalization.DateTimeStyles.None, out dDate)))
            {
                invaliddate = true;
                Response.Write("<br>End date format must be 'dd/mm/yyyy'");
            }
            else if (Convert.ToDateTime(commence) > Convert.ToDateTime(end))
            {
                invaliddate = true;
                Response.Write("<br>Commence date must come before end date");
            }
            return invaliddate;
        }

        // Function runs several valdations to ensure CompName inputs are valid and outputs a boolean value
        bool VALIDATE_COMPETITOR(string comp)
        {
            bool invalidcomp = false;

            if (string.IsNullOrEmpty(comp))
            {
                invalidcomp = true;
                Response.Write("<br>Competitor field must not be blank");
            }
            else if (!Regex.IsMatch(comp, @"^[a-zA-Z\s]+$"))
            {
                invalidcomp = true;
                Response.Write("<br>Competitor field must contain letters");
            }
            else if ((comp.Length) < 5)
            {
                invalidcomp = true;
                Response.Write("<br>Competitor field must at least 5 characters long");
            }
            return invalidcomp;
        }

        // Function runs 2 validations to ensure medal input is correct and outputs a boolean value
        bool VALIDATE_MEDAL(string Medal)
        {
            bool invalidmedal = false;

            if (!Regex.IsMatch(Medal, @"^[G | S | B].*$"))
            {
                invalidmedal = true;
                Response.Write("<br>Medal field must contain characters 'G' (gold), 'S' (silver, 'B' (bronze)");
            }
            else if (string.IsNullOrEmpty(Medal))
            {
                invalidmedal = true;
                Response.Write("<br>Medal field must not be blank");
            }
            return invalidmedal;
        }

        // Function runs 2 validation to ensure record input is correct and outputs a boolean value
        bool VALIDATE_RECORD(string Record)
        {
            bool invalidrecord = false;

            if (!Regex.IsMatch(Record, @"^[Y | N]+$"))
            {
                invalidrecord = true;
                Response.Write("<br>World record field  must contain characters 'Y' (yes) or 'N' (no)");
            }
            else if (string.IsNullOrEmpty(Record))
            {
                invalidrecord = true;
                Response.Write("<br>World record field cannot be left empty");
            }
            return invalidrecord;
        }

        // function that counts the number of form rows that contain a complete or incomplete input
        // function returns a value called $rowsfilled that decides what event occurs
        int ROWCHECK()
        {
            int rowsfilled = 0;
            int startat = 1;

            for (int x = startat; x <= 25; x++)
            {
                if (!string.IsNullOrEmpty(Request.Form["CompName_R_"+x]) || !string.IsNullOrEmpty(Request.Form["MedalWon_R_"+x]) || !string.IsNullOrEmpty(Request.Form["WorldRecord_R_"+x]))
                {
                    rowsfilled++;
                }
            }
            return rowsfilled; // Outputs an integer value
        }

